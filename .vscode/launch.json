{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: cProfile",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "args": [],
      "stopOnEntry": false,
      "cwd": "${workspaceFolder}",
      "preLaunchTask": null,
      "pythonPath": "${config:python.pythonPath}",
      "debugOptions": ["RedirectOutput"],
      "externalConsole": false,
      "internalConsoleOptions": "neverOpen",
      "profiling": true
    }
  ]
}
